@prefix sh:	<http://www.w3.org/ns/shacl#>.
@prefix xsd:	<http://www.w3.org/2001/XMLSchema#>.
@prefix ex: <http://www.w3.org/2022/example#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

ex:Aircraft
	a sh:NodeShape;
	
	sh:property [
		sh:path ex:icao24;
		sh:datatype xsd:string;
		sh:name "Icao";
	];
	sh:property [
		sh:path ex:registration;
		sh:datatype xsd:string;
		sh:name "Registration";
		sh:maxCount 1;
	];
	sh:property [
		sh:path ex:aircraftModel;
		sh:datatype xsd:string;
		sh:name "AircraftModel";
		sh:maxCount 1;
	];
	sh:property [
		sh:path ex:typeCode;
		sh:datatype xsd:string;
		sh:name "TypeCode";
		sh:maxCount 1;
	];
	sh:property [
		sh:path ex:serialNumber;
		sh:datatype xsd:string;
		sh:name "SerialNumber";
		sh:maxCount 1;
	];
	sh:property [
		sh:path ex:icaoAircraftType;
		sh:datatype xsd:string;
		sh:name "IcaoAircraftType";
		sh:maxCount 1;
	];
	sh:property [
		sh:path ex:registered;
		sh:datatype xsd:localDate;
		sh:name "Registered";
		sh:maxCount 1;
	];
	sh:property [
		sh:path ex:regUntil;
		sh:datatype xsd:localDate;
		sh:name "RegUntil";
		sh:maxCount 1;
	];
	sh:property [
		sh:path ex:built;
		sh:datatype xsd:localDate;
		sh:name "Built";
		sh:maxCount 1;
	];
	sh:property [
		sh:path ex:firstFlightDate;
		sh:datatype xsd:localDate;
		sh:name "FirstFlightDate";
		sh:maxCount 1;
	];
	sh:property [
		sh:path ex:modes;
		sh:datatype xsd:boolean;
		sh:name "Modes";
		sh:maxCount 1;
	];
	sh:property [
		sh:path ex:adsb;
		sh:datatype xsd:boolean;
		sh:name "Adsb";
		sh:maxCount 1;
	];
	sh:property [
		sh:path ex:acars;
		sh:datatype xsd:boolean;
		sh:name "Acars";
		sh:maxCount 1;
	];
	sh:property [
		sh:path ex:acars;
		sh:datatype xsd:boolean;
		sh:name "Acars";
		sh:maxCount 1;
	];
	sh:property [
		sh:path ex:notes;
		sh:datatype xsd:string;
		sh:name "Notes";
		sh:maxCount 1;
	];
	sh:property [
		sh:path ex:categoryDescription;
		sh:datatype xsd:string;
		sh:name "CategoryDescription";
		sh:maxCount 1;
	];
	sh:property [
		sh:path ex:owner;
		sh:datatype xsd:string;
		sh:name "Owner";
		sh:maxCount 1;
	];
	sh:property [
		sh:path ex:engine;
		sh:datatype xsd:string;
		sh:name "Engine";
		sh:maxCount 1;
	];
	sh:property [
          	sh:path ex:manufacturer ;
          	sh:node ex:Manufacturer ;
          	sh:maxCount 1;
      	];
	sh:property [
          	sh:path ex:operator ;
          	sh:node ex:Operator ;
          	sh:maxCount 1;
      	].


ex:Manufactorer
	a sh:NodeShape;
	sh:property [
		sh:path ex:manufactorerIcao;
		sh:datatype xsd:string;
		sh:name "ManufactorerIcao";
		sh:maxCount 1;
	];
	sh:property [
		sh:path ex:manufactorerName;
		sh:datatype xsd:string;
		sh:name "ManufactorerName";
		sh:maxCount 1;
	].

ex:Operator
	a sh:NodeShape;
	sh:property [
		sh:path ex:icao;
		sh:datatype xsd:string;
		sh:name "Icao";
		sh:maxCount 1;
	];
	sh:property [
		sh:path ex:iata;
		sh:datatype xsd:string;
		sh:name "Iata";
		sh:maxCount 1;
	];
	sh:property [
		sh:path ex:name;
		sh:datatype xsd:string;
		sh:name "Name";
		sh:maxCount 1;
	];
	sh:property [
		sh:path ex:callsign;
		sh:datatype xsd:string;
		sh:name "Callsign";
		sh:maxCount 1;
	].
